<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="place">
  
  <select id="selectAll" resultType="placeVO">
  	SELECT * FROM PLACE
  </select>
  
  <insert id="insertOne" parameterType="placeVO">
  	INSERT INTO PLACE 
  	VALUES (place_no_seq.nextval,#{address},#{place_name})
  </insert>
  
  <delete id="deleteOne" parameterType="int">
  	DELETE FROM PLACE
  	WHERE PLACE_NO = #{place_no}
  </delete>
  
  <update id="updateOne" parameterType="placeVO">
  	UPDATE PLACE
  	SET ADDRESS=#{address}, PLACE_NAME=#{place_name}
  	WHERE PLACE_NO=#{place_no}
  </update>
		  
  
  </mapper>